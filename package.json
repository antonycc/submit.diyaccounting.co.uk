{
  "name": "web-submit-diyaccounting-co-uk",
  "version": "0.0.2-4",
  "description": "Submit UK tax information to HMRC",
  "type": "module",
  "main": "app/bin/main.js",
  "bin": {
    "submit": "app/bin/main.js"
  },
  "scripts": {
    "build": "./mvnw clean package",
    "formatting": "prettier --check . && ./mvnw spotless:check",
    "formatting-fix": "prettier --write . && ./mvnw spotless:apply",
    "formatting:js": "prettier --check .",
    "formatting:js-fix": "prettier --write .",
    "formatting:java": "./mvnw spotless:check",
    "formatting:java-fix": "./mvnw spotless:apply",
    "linting": "eslint .",
    "linting-json": "eslint --format=@microsoft/eslint-formatter-sarif .",
    "linting-fix": "eslint --fix .",
    "docker:build": "docker build -t submit-base:latest -f Dockerfile .",
    "cdk:synth-application": "cd cdk-application && npx dotenv -e ../.env.${ENV_NAME:-prod} -- npx cdk synth --require-approval never --ci true --strict --validation --exclusively ${ENV_NAME:-prod}-ObservabilityStack ${ENV_NAME:-prod}-DevStack ${ENV_NAME:-prod}-IdentityStack ${ENV_NAME:-prod}-AuthStack ${ENV_NAME:-prod}-ApplicationStack ${ENV_NAME:-prod}-OpsStack ${ENV_NAME:-prod}-SelfDestructStack && cd ..",
    "cdk:synth-delivery":    "cd cdk-delivery &&    npx dotenv -e ../.env.${ENV_NAME:-prod} -- npx cdk synth --require-approval never --ci true --strict --validation --exclusively ${ENV_NAME:-prod}-EdgeStack ${ENV_NAME:-prod}-PublishStack ${ENV_NAME:-prod}-SelfDestructStack && cd ..",
    "cdk": "./mvnw clean package -DskipTests && ENV_NAME=prod npm run cdk:synth-application && ENV_NAME=prod npm run cdk:synth-delivery && npm run clean && ./mvnw clean compile -DskipTests",
    "cdk-ci": "./mvnw clean package -DskipTests && ENV_NAME=ci npm run cdk:synth-application && ENV_NAME=ci npm run cdk:synth-delivery && npm run clean && ./mvnw clean compile -DskipTests",
    "update-to-minor": "npx npm-check-updates --upgrade --enginesNode --target minor --verbose --install always",
    "update-to-greatest": "npx npm-check-updates --upgrade --enginesNode --target greatest --verbose --install always --reject 'alpha'",
    "convert:video": "node app/bin/convert.js",
    "convert:video:default": "node app/bin/convert.js --in target/behaviour-test-results/video.webm --out target/behaviour-test-results/video.mp4",
    "test": "vitest --run app/unit-tests/*.test.js web/unit-tests/*.test.js app/integration-tests/*.test.js",
    "test:all": "vitest --run app/unit-tests/*.test.js app/integration-tests/*.test.js app/system-tests/*.test.js && playwright test --project=browser-tests web/browser-tests/*.test.js && playwright test --project=behaviour-tests behaviour-tests/*.test.js",
    "test:app-unit": "vitest --run app/unit-tests/*.test.js",
    "test:web-unit": "vitest --run web/unit-tests/*.test.js",
    "test:unit": "vitest --run app/unit-tests/*.test.js web/unit-tests/*.test.js",
    "test:coverage": "vitest --run --coverage app/unit-tests/*.test.js",
    "test:integration": "vitest --run app/integration-tests/*.test.js",
    "test:system": "vitest --run app/system-tests/*.test.js",
    "test:browser": "playwright test --project=browser-tests web/browser-tests/*.test.js",
    "test:behaviour": "playwright test --project=behaviour-tests behaviour-tests/*.test.js",
    "playwright:install": "npx playwright install chromium --with-deps",
    "users:provision": "node app/bin/provision-user.mjs",
    "test:submitVatBehaviourProxyRunning": "npx dotenv -e .env.proxyRunning -- playwright test behaviour-tests/submitVat.behaviour.test.js",
    "test:submitVatBehaviour": "playwright test behaviour-tests/submitVat.behaviour.test.js",
    "test:userJourneysBehaviour": "playwright test behaviour-tests/journeys.behaviour.test.js",
    "test:zBundlesBehaviour": "playwright test behaviour-tests/zBundles.behaviour.test.js",
    "clean": "./scripts/clean-tests.sh",
    "start": "npx dotenv -e .env.proxy -- node app/bin/server.js",
    "proxy": "npx dotenv -e .env.proxy -- npx ngrok http --url wanted-finally-anteater.ngrok-free.app",
    "auth": "npx dotenv -e .env.proxy -- docker run -p 8080:8080 -e JSON_CONFIG_PATH=/config/mock-oauth2-config.json -e LOG_LEVEL=DEBUG -v $(pwd)/mock-oauth2-config.json:/config/mock-oauth2-config.json:ro ghcr.io/navikt/mock-oauth2-server:2.2.1",
    "storage": "npx dotenv -e .env.proxy -- node app/bin/minio.js"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.890.0",
    "@aws-sdk/client-secrets-manager": "^3.890.0",
    "@iarna/toml": "^3.0.0",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^9.0.0",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "toml": "^3.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.890.0",
    "@aws-sdk/lib-dynamodb": "^3.890.0",
    "@executeautomation/playwright-mcp-server": "^1.0.6",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@openai/codex": "^0.36.0",
    "@playwright/test": "^1.55.0",
    "@vitest/coverage-v8": "^3.2.4",
    "aws-cdk": "^2.1029.1",
    "aws-sdk-client-mock": "^4.1.0",
    "bcryptjs": "^3.0.2",
    "eslint": "^9.35.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "happy-dom": "^18.0.1",
    "msw": "^2.11.2",
    "ngrok": "^5.0.0-beta.2",
    "npm-check-updates": "^18.1.1",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "testcontainers": "^11.5.1",
    "uuid": "^13.0.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "package.json"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "homepage": "https://submit.diyaccounting.co.uk/"
}
