# .env.prod
#
# Features:
#   Real production DNS entry.
#   HMRC production API. (currently test API)
#   DynamoDB storage.
#   Cognito auth.
DIY_SUBMIT_ENV_FILEPATH=.env.prod
DOTENV_CONFIG_QUIET=true
ENVIRONMENT_NAME=prod
# DEPLOYMENT_NAME resolved from SSM by deploy-app.js (e.g. prod-cbf0475)
DIY_SUBMIT_BASE_URL=https://submit.diyaccounting.co.uk/

LOG_TO_CONSOLE=true
LOG_TO_FILE=true
CLOUD_TRAIL_ENABLED=true
# Security Hub and GuardDuty are account-level singletons - enable only for prod
SECURITY_SERVICES_ENABLED=true

# Cross-account Route53: assume this role in the root account to manage DNS records
ROOT_ROUTE53_ROLE_ARN=arn:aws:iam::887764105431:role/root-route53-record-delegate

HMRC_BASE_URI=https://to0request0from0hmrc-api.service.hmrc.gov.uk
# NOTE: Production HMRC client ID - public identifier, not a secret. Secret stored in AWS Secrets Manager.
HMRC_CLIENT_ID=to0request0from0hmrc
HMRC_SANDBOX_BASE_URI=https://test-api.service.hmrc.gov.uk
# NOTE: HMRC_SANDBOX_CLIENT_ID is a test application credential for HMRC Sandbox API - not a production secret
HMRC_SANDBOX_CLIENT_ID=uqMHA6RsDGGa7h8EG2VqfqAmv4tV
GOOGLE_CLIENT_SECRET_ARN=arn:aws:secretsmanager:eu-west-2:972912397388:secret:prod/submit/google/client_secret
