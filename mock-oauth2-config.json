{
  "interactiveLogin": true,
  "tokenCallbacks": [
    {
      "issuerId": "default",
      "tokenExpiry": 3600,
      "requestMappings": [
        {
          "requestParam": "code",
          "match": ".*",
          "claims": {
            "sub": "user",
            "email": "user@example.com",
            "aud": ["debugger"],
            "scope": "openid somescope",
            "nonce": "5678",
            "amr": ["mfa", "pwd"]
          }
        }
      ]
    }
  ],
  "clients": [
    {
      "clientId": "debugger",
      "clientSecret": "someSecret",
      "redirectUris": [
        "http://127.0.0.1:3000/auth/loginWithMockCallback.html",
        "https://wanted-finally-anteater.ngrok-free.app/auth/loginWithMockCallback.html"
      ],
      "grantTypes": ["authorization_code"],
      "responseTypes": ["code"],
      "tokenEndpointAuthMethod": "client_secret_basic",
      "scopes": ["openid", "somescope"]
    }
  ]
}
