{
  "interactiveLogin": true,
  "tokenCallbacks": [
    {
      "issuerId": "debugger",
      "tokenExpiry": 3600,
      "requestMappings": [
        {
          "requestParam": "code",
          "claims": {
            "sub": "user",
            "email": "user@example.com",
            "aud": [
              "debugger"
            ],
            "scope": "openid somescope",
            "nonce": "5678"
          }
        }
      ]
    }
  ],
  "clients": [
    {
      "clientId": "debugger",
      "clientSecret": "someSecret",
      "redirectUris": [
        "http://127.0.0.1:3000/loginCallback.html",
        "https://wanted-finally-anteater.ngrok-free.app/loginCallback.html"
      ],
      "grantTypes": [
        "authorization_code"
      ],
      "responseTypes": [
        "code"
      ],
      "tokenEndpointAuthMethod": "client_secret_basic",
      "scopes": [
        "openid",
        "somescope"
      ]
    }
  ]
}
